<html>
<head>
    <title>Write Up - Cadastro</title>
    <script src="main_js"></script>
    <link rel="stylesheet" type="text/css" href="main_css">
</head>
<body>
    <div class="container">
        <div class="fields">
            <section class="fields-left">
                <form id="registration_form" class="fields-form">
                    <div class="input-box">
                        <label for="name">Digite seu nome</label>
                        <input type="text" id="name" name="name">
                    </div>
                    <div class="input-box">
                        <label for="user">Digite seu usuario</label>
                        <input type="text" name="user" id="user" autocomplete="off">
                    </div>
                    <div class="input-box">
                        <label for="genre">Selecione seu genero</label>
                        <select id="genre" name="genre">
                            <option value="0"></option>
                            <option value="1">Masculino</option>
                            <option value="2">Feminino</option>
                        </select>
                    </div>
                    <div class="input-box">
                        <label for="password">Digite sua senha</label>
                        <input type="password" name="password" id="password" autocomplete="off">
                    </div>
                    <div class="input-box">
                        <label for="confirm_password">Confirme sua senha</label>
                        <input type="password" name="confirm_password" id="confirm_password">
                    </div>
                    <button class="btn-black" id="btn_submit" onclick="registrationModule.signupProcess(event)">Cadastrar</button>
                    <div class="alert-container">
                        <div class="alert-box">
                            <span id="alert" class="alert hidden"></span>
                        </div>
                    </div>
                </form>
            </section>
            <section class="fields-right" style="overflow: hidden;">
                Imagem
            </section>
        </div>
    </div>
</body>
</html>